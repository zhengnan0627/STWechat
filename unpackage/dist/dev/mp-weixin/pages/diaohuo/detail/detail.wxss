
page{
	background-color: #EEEEEE !important;
}
.container {
	width: 100vw;
	overflow: hidden;
	background-color: #EEEEEE;
}
.detail-info-box{
	width: 100vw;
	height: 400rpx;
	background-color: #FFFFFF;
	margin-bottom: 10rpx;
}
.detail-info-state{
	box-sizing: border-box;
	padding: 0rpx 20rpx;
	width: 100%;
	height: 200rpx;
	line-height: 200rpx;
	background-color: #1296DB;
	font-size: 32rpx;
	color: #FFFFFF;
}
.detail-info-dingdan{
	box-sizing: border-box;
	height: 200rpx;
	padding: 10rpx 20rpx;
	background-color: #FFFFFF;
	overflow: hidden;
	font-size: 28rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.detailinfo-box-line{
	box-sizing: border-box; 
	padding: 6rpx 0;
}
.detail-list-box{
	box-sizing: border-box;
	padding: 10rpx 20rpx;
	background-color: #FFFFFF;
	overflow: hidden;
	font-size: 28rpx;
	border-bottom: 1px solid #EEEEEE;
}
.detail-list-box:nth-of-type(2){
	margin-top: 20rpx;
	/* background-color: #007AFF; */
}
.list-box-line{
	box-sizing: border-box; 
	padding: 6rpx 0;
}
.list-box-line-text{
	color: #999999;
	margin-right: 10rpx;
}
.list-box-flex{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	min-height: 60rpx;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}

